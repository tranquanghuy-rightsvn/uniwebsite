<div class="container">
  <%= link_to "<button class='btn btn-success mb-3'> Tạo User</button>".html_safe, new_website_user_path(website_id: params[:website_id]) %>
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th scope="col">Email</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% @users.map do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><button class="btn btn-danger">Xóa</button></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
